{
    "openapi": "3.0.0",
    "info": {
        "version": "0.0.1",
        "title": "Icon1 API",
        "license": {
            "name": "UNLICENSED"
        }
    },
    "servers": [
        {
            "url": "http://localhost:3030",
            "description": "Local dev server"
        }
    ],
    "paths": {
        "/": {
            "get": {
                "summary": "home",
                "operationId": "home",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "Home"
                    }
                }
            }
        },
        "/icons": {
            "get": {
                "summary": "Icons Provider List",
                "operationId": "icons-provider-list",
                "x-google-backend": {
                    "address": "%GCF_URL%"
                },
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "Listing providers"
                    }
                }
            }
        },
        "/icons/{provider}": {
            "get": {
                "summary": "Icons List",
                "operationId": "icons-list",
                "parameters": [],
                "x-google-backend": {
                    "address": "%GCF_URL%"
                },
                "responses": {
                    "200": {
                        "description": "Listing Icons of a provider"
                    }
                }
            }
        },
        "/icon/{provider}/{icon}": {
            "get": {
                "summary": "Icon View",
                "operationId": "icon-view",
                "parameters": [],
                "x-google-backend": {
                    "address": "%GCF_URL%"
                },
                "responses": {
                    "200": {
                        "description": "View an Icon as JSON or as SVG"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Error": {
                "type": "object",
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "integer"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
